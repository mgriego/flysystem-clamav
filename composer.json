{
    "name": "mgriego/flysystem-clamav",
    "description": "The Flysystem ClamAV scanner acts as a passthrough filesystem, scanning files that are retrieved from or stored to a backing Flysystem adapter (ie Local, FTP, S3, etc).",
    "type": "library",
    "require": {
        "php": ">=5.6.0",
        "league/flysystem": "^1.0",
        "xenolope/quahog": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "mgriego\\Flysystem\\ClamAV\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Griego",
            "email": "mike@griegocentral.com"
        }
    ],
    "minimum-stability": "stable"
}
